{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "python:install": "pip install -r src/yahoo-client/utility/python/common/requirements.txt",
    "start:dev": "npm run python:install && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "create-pr-description": "bash .github/pr_workflows/create_pr.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:all": "npm run test && pytest",
    "test:stock-price-repository": "jest src/yahoo-client/stock/tests/stock-price.repository.test.ts",
    "test:stock-report-repository": "jest src/yahoo-client/stock/tests/stock-report.repository.test.ts",
    "test:stock-price-service": "jest src/yahoo-client/stock/stock-price/__tests__/stock-price.service.spec.ts",
    "test:stock-report-service": "jest src/yahoo-client/stock/stock-report/__tests__/stock-report.service.spec.ts",
    "test:stock-repositories": "npm run test:stock-price-repository && npm run test:stock-report-repository",
    "test:stock-services": "npm run test:stock-price-service && npm run test:stock-report-service",
    "test:big-five-controller": "jest src/yahoo-client/analysis/big-five-analysis/big-five-analysis.controller.spec.ts",
    "test:big-five-service": "jest src/yahoo-client/analysis/big-five-analysis/big-five-analysis.service.spec.ts",
    "test:big-five-service-integration": "jest src/yahoo-client/analysis/big-five-analysis/big-five-analysis.service.integration.spec.ts",
    "test:big-five-msft-real-data": "jest src/yahoo-client/analysis/big-five-analysis/msft-real-data.integration.spec.ts",
    "test:analysis-service": "npm run test:big-five-controller && npm run test:big-five-service",
    "test:analysis-integration": "npm run test:big-five-service-integration && npm run test:big-five-msft-real-data",
    "test:python-data-manager": "pytest src/yahoo-client/utility/python/tests/test_data_manager.py"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^8.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parser": "^3.0.0",
    "express": "^5.1.0",
    "mongoose": "^8.8.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
